<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="org.antlr.intellij.plugin.preview.InputPanel">
  <grid id="27dc6" binding="outerMostPanel" custom-create="true" layout-manager="BorderLayout" hgap="10" vgap="10">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
