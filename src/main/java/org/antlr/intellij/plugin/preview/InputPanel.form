          </component>
          <component id="e1a4f" class="javax.swing.JLabel" binding="startRuleLabel2">
            <constraints/>
            <properties>
              <text value="Label"/>
            </properties>
          </component>
          <component id="bbb1e" class="com.intellij.openapi.ui.ComboBox" binding="comboBox" custom-create="true">
            <constraints/>
            <properties/>
          </component>
        </children>
      </grid>
      <component id="4d6c1" class="javax.swing.JTextArea" binding="placeHolder">
        <constraints border-constraint="East"/>
        <properties>
          <background awt-color="lightGray"/>
          <editable value="false"/>
          <enabled value="true"/>
          <margin top="0" left="0" bottom="0" right="0"/>
